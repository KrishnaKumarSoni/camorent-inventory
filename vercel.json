{
  "version": 2,
  "name": "camo-inv",
  "alias": ["camo-inv.vercel.app"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/",
      "dest": "/build/index.html"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  },
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  }
}